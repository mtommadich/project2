
{{# each documents}}
  <div class="row">
    {{# each this }}
    <div class="col-sm-6 col-md-4">
      <div class="thumbnail">
        <span style="font-size: 10em; margin-left: .5em; color: red;">  
         {{{ this.iconPath }}}
        </span>
        <!-- img src="..." alt="pdf icon"-->
        <div class="caption">
          <h3>{{this.title}}</h3>
          <p class="description">{{this.description}}</p>
          <div class="clearfix"><a href="#" class="btn btn-primary btn-rounded" role="button"><i class="fa fa-download" aria-hidden="true"></i></a>
          </div>
        </div>
      </div>
    </div> 
    {{/each}}
  </div>  
{{/each}}


